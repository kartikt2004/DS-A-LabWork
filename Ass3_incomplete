#include <iostream>
#include <string.h>
using namespace std;

struct node {

    string name;
    string artist;
    node * nxt;
};
int main() {
    node * head;
    node * p = new node;
    p -> data = 10;
    p -> nxt = NULL;
    
    if (head == NULL) {
        head = p;
    }
    else {
        q = head;
        while (q -> nxt != NULL) {
            q = q -> nxt;
        }
        q -> nxt = p;
    }
    

    return 0;
}
